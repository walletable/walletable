{
    "name": "walletable/walletable",
    "description": "A package to create e-wallets for managing and analysing digital financial assets per entity in your laravel application.",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/walletable/walletable",
    "support": {
        "issues": "https://github.com/walletable/walletable/issues",
        "source": "https://github.com/walletable/walletable"
    },
    "authors": [
        {
            "name": "Ilesanmi Olawale Adedotun",
            "homepage": "https://twitter.com/mane_olawale",
            "email": "horlwaley001@gmail.com"
        }
    ],
	"require": {
		"php": ">=7.1",
        "ext-intl": "*",
        "laravel/framework": "^7.0|^8.0|^9.0|^10.0|^11.0"
	},
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0",
        "phpunit/phpunit": "~8.0|~9.0",
        "mockery/mockery": "~1.4"
    },
    "autoload": {
        "psr-4": {
            "Walletable\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
         "psr-4": {
            "Walletable\\Tests\\": "tests/"
         }
     },
     "scripts": {
         "test": "./vendor/bin/phpunit --verbose"
     },
    "extra": {
        "laravel": {
            "providers": [
                "Walletable\\WalletableServiceProvider"
            ]
        }
    }
}